        }
      },
      "displays": [
        {
          "type": "LinearBasicDisplay",
          "displayId": "other_uniprot_proteins-$ASSEMBLY-LinearBasicDisplay",
          "renderer": {
            "type": "SvgFeatureRenderer",
            "color1": "orange",
            "height": 4,
            "showLabels": false,
            "showDescriptions": false
          }
        }
      ]
    }
  ]
