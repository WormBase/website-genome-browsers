{
  "tracks": [
    {
      "type": "FeatureTrack",
      "trackId": "$ASSEMBLY_D_melanogaster_proteins",
      "name": "D. melanogaster proteins",
      "assemblyNames": [
        "$ASSEMBLY"
      ],
      "description": "Matches to WormBase D. melanogaster proteins, aligned with BLASTX (nucleotide to protein, via six-frame translation).",
      "category": [
        "Sequence Similarity",
        "Proteins"
      ],
      "adapter": {
        "type": "NCListAdapter",
        "rootUrlTemplate": {
          "locationType": "UriLocation",
          "uri": "https://s3.amazonaws.com/agrjbrowse/MOD-jbrowses/WormBase/WS$RELEASE/$ASSEMBLY/tracks/D. melanogaster proteins/{refseq}/trackData.jsonz"
        }
      },
      "displays": [
        {
          "type": "LinearBasicDisplay",
          "displayId": "D_melanogaster_proteins-$ASSEMBLY-LinearBasicDisplay",
          "mouseover": "jexl:get(feature,'clone')",
          "renderer": {
            "type": "SvgFeatureRenderer",
            "color1": "orange",
            "height": 4,
            "showLabels": false
          }
        }
      ]
    }
  ]
}
