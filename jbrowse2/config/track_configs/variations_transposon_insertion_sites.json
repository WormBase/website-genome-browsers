{
  "tracks": [
    {
      "type": "FeatureTrack",
      "trackId": "$ASSEMBLY_transposon_insert_sites",
      "name": "Transposon insert sites",
      "assemblyNames": [
        "$ASSEMBLY"
      ],
      "description": "This track shows transposon insertion sites engineered by Laurent Segalat and others: 'Towards a genome-wide collection of transposon insertions', International C. elegans Meeting 1999 Yellow triangles are Mos-derived transposon insertions; red trangles are NemaGENETAG consortium insertion sites; blue triangles are Tc* derived transposon insertions.",
      "formatDetails": {
         "feature": "jexl:{ JBrowse1: '<a href=\"https://stage.alliancegenome.org/jbrowse/worms/jbrowse-simple/?data=data/$ASSEMBLY' + '&loc=' +feature.refName+':'+feature.start+'..'+feature.end+ '\">'+feature.refName+':'+feature.start+'..'+feature.end+'</a>'}"
      },
      "category": [
        "Alleles, Variations, RNAi"
      ],
      "adapter": {
        "type": "NCListAdapter",
        "rootUrlTemplate": {
          "locationType": "UriLocation",
          "uri": "https://s3.amazonaws.com/agrjbrowse/MOD-jbrowses/WormBase/WS$RELEASE/$ASSEMBLY/tracks/Transposon insert sites/{refseq}/trackData.jsonz"
        }
      },
      "displays": [
        {
          "type": "LinearBasicDisplay",
          "displayId": "transposon_insert_sites-$ASSEMBLY-LinearBasicDisplay",
          "renderer": {
            "type": "SvgFeatureRenderer",
            "color1": "jexl:get(feature,'source')=='Mos_insertion_allele'?'yellow':get(feature,'source')=='NemaGENETAG_consortium'?'red':'blue'",
            "height": 8
          }
        }
      ]
    }
  ]
}
