{
  "tracks": [
    {
      "type": "FeatureTrack",
      "trackId": "$ASSEMBLY_collapsed_isoseq_structures",
      "name": "Collapsed IsoSeq structures",
      "assemblyNames": [
        "$ASSEMBLY"
      ],
      "description": "Aligned and filtered full-length non-chimeric IsoSeq reads with similar alignments collapsed into one representative structure.",
      "category": [
        "Genes",
        "Supporting Evidence"
      ],
      "adapter": {
        "type": "NCListAdapter",
        "rootUrlTemplate": {
          "locationType": "UriLocation",
          "uri": "https://s3.amazonaws.com/agrjbrowse/MOD-jbrowses/WormBase/WS$RELEASE/$ASSEMBLY/tracks/Collapsed IsoSeq structures/{refseq}/trackData.jsonz"
        }
      },
      "displays": [
        {
          "type": "LinearBasicDisplay",
          "displayId": "collapsed_isoseq_structures-$ASSEMBLY-LinearBasicDisplay",
          "renderer": {
            "type": "SvgFeatureRenderer",
            "color1": "jexl:get(feature,'source')=='BLAT_Caen_mRNA_BEST'?'lightblue':'green'",
            "height": 4,
            "showLabels": false,
            "showDescriptions": false
          }
        }
      ]
    }
  ]
}
